{
  "name": "@eveworld/world",
  "version": "0.0.21",
  "license": "MIT",
  "scripts": {
    "build": "pnpm run build:mud && pnpm run build:abi && pnpm run build:abi-ts",
    "build:abi": "forge build --silent",
    "build:abi-ts": "mud abi-ts",
    "build:mud": "rm -rf src/codegen && mud tablegen && mud worldgen",
    "deploy": "ERC20_TOKEN_NAME=$ERC20_TOKEN_NAME && ERC20_TOKEN_SYMBOL=$ERC20_TOKEN_SYMBOL && ERC20_INITIAL_SUPPLY=$ERC20_INITIAL_SUPPLY && EVE_TOKEN_NAMESPACE=$EVE_TOKEN_NAMESPACE && EVE_TOKEN_ADMIN=$EVE_TOKEN_ADMIN && PRIVATE_KEY=$PRIVATE_KEY mud deploy --rpc $RPC_URL --alwaysRunPostDeploy",
    "upgrade": "PRIVATE_KEY=$PRIVATE_KEY mud deploy --rpc $RPC_URL --worldAddress $WORLD_ADDRESS",
    "clean": "rm -rf src/codegen && rm -rf out && rm -rf cache",
    "dev": "pnpm mud dev-contracts",
    "lint": "pnpm run prettier && pnpm run solhint",
    "prettier": "prettier 'src/**/*.sol' 'test/**/*.sol' './*.ts' --plugin=prettier-plugin-solidity --check",
    "fixFormat": "prettier 'src/**/*.sol' 'test/**/*' './*.ts' --plugin=prettier-plugin-solidity --write",
    "solhint": "solhint --config ./.solhint.json 'src/**/*.sol'",
    "test": "tsc --noEmit && mud test --forgeOptions=-vv",
    "access-config:access-lists": "forge script script/AccessListConfig.s.sol:AccessListConfig --private-key $PRIVATE_KEY --chain-id $CHAIN_ID --broadcast --rpc-url $RPC_URL --sig \"run(address)\" $WORLD_ADDRESS -vvv",
    "access-config:entity-record": "forge script script/EntityRecordAccessConfig.s.sol:EntityRecordAccessConfig --private-key $PRIVATE_KEY --chain-id $CHAIN_ID --broadcast --rpc-url $RPC_URL --sig \"run(address)\" $WORLD_ADDRESS -vvv",
    "access-config:erc-721-puppet": "forge script script/ERC721PuppetAccessConfig.s.sol:ERC721PuppetAccessConfig --private-key $PRIVATE_KEY --chain-id $CHAIN_ID --broadcast --rpc-url $RPC_URL --sig \"run(address)\" $WORLD_ADDRESS -vvv",
    "access-config:inventory": "forge script script/InventoryAccessConfig.s.sol:InventoryAccessConfig --private-key $PRIVATE_KEY --chain-id $CHAIN_ID --broadcast --rpc-url $RPC_URL --sig \"run(address)\" $WORLD_ADDRESS -vvv",
    "access-config:kill-mail": "forge script script/KillMailAccessConfig.s.sol:KillMailAccessConfig --private-key $PRIVATE_KEY --chain-id $CHAIN_ID --broadcast --rpc-url $RPC_URL --sig \"run(address)\" $WORLD_ADDRESS -vvv",
    "access-config:location": "forge script script/LocationAccessConfig.s.sol:LocationAccessConfig --private-key $PRIVATE_KEY --chain-id $CHAIN_ID --broadcast --rpc-url $RPC_URL --sig \"run(address)\" $WORLD_ADDRESS -vvv",
    "access-config:smart-character": "forge script script/SmartCharacterAccessConfig.s.sol:SmartCharacterAccessConfig --private-key $PRIVATE_KEY --chain-id $CHAIN_ID --broadcast --rpc-url $RPC_URL --sig \"run(address)\" $WORLD_ADDRESS -vvv",
    "access-config:smart-deployable": "forge script script/SmartDeployableAccessConfig.s.sol:SmartDeployableAccessConfig --private-key $PRIVATE_KEY --chain-id $CHAIN_ID --broadcast --rpc-url $RPC_URL --sig \"run(address)\" $WORLD_ADDRESS -vvv",
    "access-config:smart-gate": "forge script script/SmartGateAccessConfig.s.sol:SmartGateAccessConfig --private-key $PRIVATE_KEY --chain-id $CHAIN_ID --broadcast --rpc-url $RPC_URL --sig \"run(address)\" $WORLD_ADDRESS -vvv",
    "access-config:smart-storage-unit": "forge script script/SmartStorageUnitAccessConfig.s.sol:SmartStorageUnitAccessConfig --private-key $PRIVATE_KEY --chain-id $CHAIN_ID --broadcast --rpc-url $RPC_URL --sig \"run(address)\" $WORLD_ADDRESS -vvv",
    "access-config:smart-turret": "forge script script/SmartTurretAccessConfig.s.sol:SmartTurretAccessConfig --private-key $PRIVATE_KEY --chain-id $CHAIN_ID --broadcast --rpc-url $RPC_URL --sig \"run(address)\" $WORLD_ADDRESS -vvv",
    "access-config:static-data": "forge script script/StaticDataAccessConfig.s.sol:StaticDataAccessConfig --private-key $PRIVATE_KEY --chain-id $CHAIN_ID --broadcast --rpc-url $RPC_URL --sig \"run(address)\" $WORLD_ADDRESS -vvv",
    "access-config:configure-all": "PRIVATE_KEY=$PRIVATE_KEY && RPC_URL=$RPC_URL && CHAIN_ID=$CHAIN_ID && WORLD_ADDRESS=$WORLD_ADDRESS && ADMIN_ACCOUNTS=$ADMIN_ACCOUNTS && pnpm run access-config:access-lists && pnpm run access-config:entity-record && pnpm run access-config:erc-721-puppet && pnpm run access-config:inventory && pnpm run access-config:kill-mail && pnpm run access-config:location && pnpm run access-config:smart-character && pnpm run access-config:smart-deployable && pnpm run access-config:smart-gate && pnpm run access-config:smart-storage-unit && pnpm run access-config:smart-turret && pnpm run access-config:static-data"
  },
  "dependencies": {
    "@latticexyz/cli": "2.2.14",
    "@latticexyz/schema-type": "2.2.8",
    "@latticexyz/store": "2.2.8",
    "@latticexyz/world": "2.2.8",
    "@latticexyz/world-modules": "2.2.8",
    "@latticexyz/gas-report": "2.2.8",
    "@eveworld/smart-object-framework": "0.0.8",
    "@eveworld/common-constants": "0.0.13"
  },
  "devDependencies": {
    "@types/debug": "4.1.7",
    "@types/node": "^22.13.1",
    "@types/prettier": "2.7.2",
    "ds-test": "https://github.com/dapphub/ds-test.git#e282159d5170298eb2455a6c05280ab5a73a4ef0",
    "forge-std": "https://github.com/foundry-rs/forge-std.git#74cfb77e308dd188d2f58864aaf44963ae6b88b1",
    "prettier": "3.2.5",
    "prettier-plugin-solidity": "1.3.1",
    "solhint": "^3.3.7",
    "solhint-config-mud": "2.2.8",
    "solhint-plugin-mud": "2.2.8",
    "typescript": "^5.4.5"
  }
}